<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- 스프링 컨테이너로 클래스를 외부에서 생성하는 공간 -->
	<!-- id="빈의 고유 이름" class="객체를 생성할 클래스의 위치(패키지명.클래스명)" -->
	<!-- 스프링 컨테이너에서 빈 객체 생성하는 방법 => 빈 정의 
		 com.sist.di02.ExamDAOImpl 클래스를 daoImple 이라는 id를 지정하여 bean 객체를 생성한다는 의미
		 ExamDAOImple daoImple = new ExamDAOImpl();
	-->
	<bean id="daoImpl" class="com.sist.di02.ExamDAOImpl"></bean>

</beans>
